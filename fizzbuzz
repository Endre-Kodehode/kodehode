/*
Lager en liste fra 1-100, men skriver Fizz hvis tallet kan deles på 3,
Buzz hvis tallet kan deles på 5, og Fizz Buzz hvis tallet kan deles på 15
Egen løsning :)
*/

for(let number = 0; number < 100; number++){
    if (number % 15 == 0){
        console.log("Fizz! Buzz!")
    } else if (number % 5 == 0){
        console.log("Buzz!")
    } else if (number % 3 == 0){
        console.log("Fizz!")
    } else {
        console.log(number)
    }
}
